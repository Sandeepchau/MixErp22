@model dynamic

@{
    ViewBag.Title = I18N.VerifyExits;
    Layout = ViewBag.Layout;
}
<script>
    var scrudFactory = new Object();

    scrudFactory.title = window.translate("VerifyExits");

    scrudFactory.viewAPI = "/api/views/hrm/exit-verification-scrud-view";
    scrudFactory.viewTableName = "hrm.exit_verification_scrud_view";

    scrudFactory.formAPI = "/api/forms/hrm/exits";
    scrudFactory.formTableName = "hrm.exits";

    scrudFactory.hasVerification = true;
    scrudFactory.verificationApi = "/dashboard/hrm/verification/exits";

    scrudFactory.excludedColumns = ["AuditUserId", "AuditTs", "Photo"];
    $.get('/ScrudFactory/View.html', function (view) {
        $("#ScrudFactoryView").html(view);
        $.cachedScript("/assets/js/scrudfactory-view.js");
    });
</script>

<div id="ScrudFactoryView"></div>
